   "source": [
    "\n",
    "import re\n",
    "\n",
    "matrix = list()\n",
    "for _ in range(int(input().split()[0])):\n",
    "    matrix.append(list(input()))\n",
    "\n",
    "\n",
    "matrix = list(zip(*matrix))\n",
    "\n",
    "\n",
    "sample = str()\n",
    "for subset in matrix:\n",
    "    for letter in subset:\n",
    "        sample += letter\n",
    "\n",
    "\n",
    "print(re.sub(r'(?<=\\w)([^\\w\\d]+)(?=\\w)', ' ', sample))\n"
   ]
